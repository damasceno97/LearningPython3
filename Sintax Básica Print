#PRINTANDO LISTA ("Vetor em C")

lista = [1,2]

for i in range(len(lista)):

    print("%d\n"%(lista[i]))
    
--------------------------------------------------------------------------------------------------------------------------------------

#PRINTANDO VARIÁVEIS

a=2
b=3

c = 'casa'
d = 'dica'


print("{},{}".format(b,a))
print("{1},{0}".format(b,a)) #O "1" e o "0" funcionam como indices dentro do format
print("{},{}".format(c,d))

